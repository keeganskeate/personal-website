{
  "hosting": [
    {
      "target": "dev",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/development/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "keegan-skeate-website",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "target": "production",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/development/**"
      ],
      "rewrites": [
        {
          "source": "!/sitemap",
          "run": {
            "serviceId": "keegan-skeate-website",
            "region": "us-central1"
          }
        },
        {
          "source": "!/static",
          "run": {
            "serviceId": "keegan-skeate-website",
            "region": "us-central1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "keegan-skeate-website",
            "region": "us-central1"
          }
        }
      ]
    }
  ]
}
